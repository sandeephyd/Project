<!-- applicaton with iframe and menubar (hyperlink) -->
<!DOCTYPE html>
<html lang="en">		 
 <head>
	<title>Ex46</title>
	<style>
		/* menu bar designing */
		#menubar{			
			width:100%
			height:30px;
			background-color:#6b6b6b;
			text-align:center;
		}
		/* content box designing */
		#content{
			width:95%;
			height:700px;
			margin:auto;
			background:#ccffcc;			
		}
		/* menus designing */
		a{
			font-size:22px;
			text-decoration:none;
			display:inline-block;
			width:50px;
			color:#fde302;
		}
		a:hover{
			color:#04fd11;
		}
		iframe{
			width:100%;
			height:100%;
		}
	</style>
 </head>
 <body>
	<!-- horizontal menu bar -->
	<div id="menubar">
			<a href="ui example telegram/h12.html" target="mf">Html Colors</a>
			<a href="ui example telegram/h16.html" target="mf">UnOrder List</a>
			<a href="ui example telegram/h21.html" target="mf">Grid Layout</a>
			<a href="ui example telegram/h24.html" target="mf">Tables</a>
			<a href="ui example telegram/h28.html" target="mf">Text Fields</a>
	</div>
	<!-- content -->
	<br>
	<div id="content">
		<iframe name="mf" src="ui example telegram/h12.html" frameborder> </iframe>
	</div>
 </body>		
</html>					 
	
							
